+// START NEW PRETTY NAME HELPER FUNCTIONS
+// Cache: key = "parentPath + acidFolder"
+const accountNameCache = new Map();
+
+/**
+ * Fetch the pretty name from /files/<path>/<acid>/ _meta/account.json
+ * Returns: string | null
+ */
+async function fetchAccountName(parentPath, acidFolder) {
+  // parentPath comes in like "a/" ; ensure no duplicate slashes
+  const base = parentPath.endsWith("/") ? parentPath.slice(0, -1) : parentPath;
+  const url  = `/files/${base}/${acidFolder}/_meta/account.json`;
+
+  try {
+    const res = await fetch(url, { headers: { "Accept": "application/json" } });
+    if (!res.ok) return null;
+    const data = await res.json();
+
+    // Prefer explicit "name", then org_id, then acid_display
+    return data?.name || data?.org_id || data?.acid_display || null; // :contentReference[oaicite:1]{index=1}
+  } catch (e) {
+    console.warn("account.json fetch failed:", e);
+    return null;
+  }
+}
+
+/**
+ * Get (and memoize) the pretty name promise for this entry
+ */
+function getPrettyName(parentPath, acidFolder) {
+  const key = `${parentPath}${acidFolder}`;
+  if (!accountNameCache.has(key)) {
+    accountNameCache.set(
+      key,
+      fetchAccountName(parentPath, acidFolder).catch(() => null)
+    );
+  }
+  return accountNameCache.get(key);
+}
